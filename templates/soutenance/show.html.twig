{% extends 'base.html.twig' %}

{% block title %}Soutenance{% endblock %}

{% block body %}
    <h1>Soutenance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ soutenance.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ soutenance.date ? soutenance.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ soutenance.note }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_soutenance_index') }}">back to list</a>

    <a href="{{ path('app_soutenance_edit', {'id': soutenance.id}) }}">edit</a>

    {{ include('soutenance/_delete_form.html.twig') }}
{% endblock %}
